---
import Layout from "../layouts/Layout.astro";
import { Image } from "astro:assets";
import morskapetsVilleVeierImg from "../images/morskapets-ville-veier-monofon.jpg";
---

<Layout title="Morskapets ville veier | Lisa Nybrott">
  <main class="flex flex-col justify-center items-center h-full w-full">
    <Image
      src={morskapetsVilleVeierImg}
      alt="Morskapets ville veier"
      class="w-full max-w-xl rounded-xl"
      quality={100}
    />
  </main>
</Layout>
